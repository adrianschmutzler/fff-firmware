-			compatible = "fixed-partitions";
-			#address-cells = <1>;
-			#size-cells = <1>;
-
-			partition@0 {
-				label = "u-boot";
-				reg = <0x000000 0x020000>;
-				read-only;
-			};
-
-			partition@20000 {
-				compatible = "tplink,firmware";
-				label = "firmware";
-				reg = <0x020000 0xf30000>;
-			};
-
-			info: partition@f50000 {
-				label = "product-info";
-				reg = <0xf50000 0x020000>;
-				read-only;
-			};
-
-			partition@f70000 {
-				label = "config";
-				reg = <0xf70000 0x050000>;
