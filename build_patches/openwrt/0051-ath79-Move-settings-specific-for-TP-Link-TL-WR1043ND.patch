+++ b/target/linux/ath79/dts/qca9563_tplink_tl-wr1043n.dtsi
@@ -86,17 +86,6 @@
 			debounce-interval = <60>;
 		};
 	};
-
-	gpio-export {
-		compatible = "gpio-export";
-		#size-cells = <0>;
-
-		gpio_usb_power {
-			gpio-export,name = "tp-link:power:usb";
-			gpio-export,output = <1>;
-			gpios = <&gpio 8 GPIO_ACTIVE_HIGH>;
-		};
-	};
 };
 
 &uart {
@@ -107,65 +96,6 @@
 	status = "okay";
 };
 
