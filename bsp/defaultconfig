# This config can be selected in other targets by defaultconfig

configyes BUSYBOX_CUSTOM
configno BUSYBOX_CONFIG_BRCTL
configno BUSYBOX_CONFIG_CROND
configno BUSYBOX_CONFIG_CRONTAB
configno BUSYBOX_CONFIG_FEATURE_FAST_TOP
configno BUSYBOX_CONFIG_FEATURE_NTPD_SERVER
configyes CLEAN_IPKG
configno DROPBEAR_CURVE25519
configno FASTD_ENABLE_CIPHER_SALSA2012
configno FASTD_ENABLE_MAC_GHASH
configno FASTD_ENABLE_MAC_UHASH
configno FASTD_ENABLE_METHOD_COMPOSED_GMAC
configno FASTD_ENABLE_METHOD_COMPOSED_UMAC
configno FASTD_ENABLE_METHOD_GENERIC_GMAC
configno FASTD_ENABLE_METHOD_GENERIC_UMAC
configno KERNEL_DEBUG_INFO
configno KERNEL_DEBUG_KERNEL
configno PACKAGE_ALFRED_VIS

packagemod opkg
